#include "mode.h"

Mode clock_mode;
Mode alarm_mode;
Mode stopwatch_mode;
Mode timer_mode;

Mode* get_clock_mode(void)
{
	return &clock_mode;
}

Mode* get_alarm_mode(void)
{
	return &alarm_mode;
}

Mode* get_stopwatch_mode(void)
{
	return &stopwatch_mode;
}

Mode* get_timer_mode(void)
{
	return &timer_mode;
}
