#ifndef SRC_LED_C_
#define SRC_LED_C_

#include "led.h"

static void toggle_left(Led* this)
{
	this->state = HAL_GPIO_ReadPin(this->GPIOx, this->GPIO_Pin);
	this->state ^= 1;
	HAL_GPIO_WritePin(this->GPIOx, this->GPIO_Pin, this->state);
}

static void toggle_right(Led* this)
{
	this->state = ~HAL_GPIO_ReadPin(this->GPIOx, this->GPIO_Pin);
	this->state ^= 1;
	HAL_GPIO_WritePin(this->GPIOx, this->GPIO_Pin, this->state);
}

Led led_left_red



#endif /* SRC_LED_C_ */
